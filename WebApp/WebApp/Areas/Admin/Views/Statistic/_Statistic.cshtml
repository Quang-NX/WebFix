
<div class="list_of_members">
    <div class="sales">
        <div class="icon">
            <i class="dollar"></i>
        </div>
        <div class="icon-text">
            @{
                double QuantityTotalPrice = double.Parse(ViewBag.QuantityTotalPrice);
            }
            <h3>@QuantityTotalPrice.ToString("##,#")</h3>
            <p>Tổng doanh thu</p>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="new-users">
        <div class="icon">
            <i class="user1"></i>
        </div>
        <div class="icon-text">
            <h3>@ViewBag.QuantityUser</h3>
            <p>Thành viên</p>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="orders">
        <div class="icon">
            <i class="order"></i>
        </div>
        <div class="icon-text">
            <h3>@ViewBag.QuantityOrder</h3>
            <p>Tổng đơn hàng</p>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="visitors">
        <div class="icon">
            <i class="visit"></i>
        </div>
        <div class="icon-text">
            <h3>@ViewBag.QuantityOnline</h3>
            <p>Khách ghé thăm</p>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="clearfix"></div>
</div>